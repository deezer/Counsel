<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.deezer.android.counsel.sample">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>


        <activity
            android:name=".CachedResultActivity"
            android:label="Cached Result">
            <intent-filter>
                <action android:name="com.deezer.android.counsel.SAMPLE_ACTION"/>
            </intent-filter>
        </activity>

        <activity
            android:name=".MultiThreadActivity"
            android:label="Multithreading">
            <intent-filter>
                <action android:name="com.deezer.android.counsel.SAMPLE_ACTION"/>
            </intent-filter>
        </activity>

        <activity
            android:name=".RetryActivity"
            android:label="Retry">
            <intent-filter>
                <action android:name="com.deezer.android.counsel.SAMPLE_ACTION"/>
            </intent-filter>
        </activity>

        <activity
            android:name=".PoolableActivity"
            android:label="Object Pool">
            <intent-filter>
                <action android:name="com.deezer.android.counsel.SAMPLE_ACTION"/>
            </intent-filter>
        </activity>
    </application>

</manifest>